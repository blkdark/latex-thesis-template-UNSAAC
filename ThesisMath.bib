@article{geoffrionConstructValidityProfessional2019,
  title = {Construct {{Validity}} of the {{Professional Quality}} of {{Life}} ({{ProQoL}}) {{Scale}} in a {{Sample}} of {{Child Protection Workers}}},
  author = {Geoffrion, Steve and Lamothe, Josianne and Morizot, Julien and Giguère, Charles-Édouard},
  date = {2019-08},
  journaltitle = {Journal of Traumatic Stress},
