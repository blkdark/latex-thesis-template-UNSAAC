@article{driessenEvaluationBerlinPolytrauma2021,
  title = {Evaluation of the {{Berlin}} Polytrauma Definition: {{A Dutch}} Nationwide Observational Study},
  shorttitle = {Evaluation of the {{Berlin}} Polytrauma Definition},
  author = {Driessen, M. L. S. and Sturms, Leontien M. and family=Zwet, given=Erik W., prefix=van, useprefix=false and Bloemers, Frank W. and family=Duis, given=H. J., prefix=ten, useprefix=false and Edwards, Michael J. and family=Hartog, given=Dennis, prefix=den, useprefix=false and family=Jongh, given=Mariska A. C., prefix=de, useprefix=false and Leenhouts, Peter A. and Poeze, Martijn and Schipper, Inger B. and Spanjersberg, Richard W. and Wendt, Klaus W. and family=Wit, given=Ralph J., prefix=de, useprefix=false and family=Zutphen, given=Stefan W. A. M., prefix=van, useprefix=false and Leenen, Luke P. H.},
  date = {2021},
  journaltitle = {Journal of Trauma and Acute Care Surgery},
  volume = {90},
  number = {4},
  doi = {10.1097/ta.0000000000003071},
  url = {https://doi.org/10.1097/ta.0000000000003071},
  urldate = {2024-03-01},
  abstract = {BACKGROUND:The Berlin polytrauma definition (BPD) was established to identify multiple injury patients with a high risk of mortality. The definition includes injuries with an Abbreviated Injury Scale score of ≥3 in ≥2 body regions (2AIS ≥3) combined with the presence of ≥1 physiological risk factors (PRFs). The PRFs are based on age, Glasgow Coma Scale, hypotension, acidosis, and coagulopathy at specific cutoff values. This study evaluates and compares the BPD with two other multiple injury definitions used to identify patients with high resource utilization and mortality risk, using data from the Dutch National Trauma Register (DNTR). METHODS:The evaluation was performed based on 2015 to 2018 DNTR data. First, patient characteristics for 2AIS ≥3, Injury Severity Score (ISS) of ≥16, and BPD patients were compared. Second, the PRFs prevalence and odds ratios of mortality for 2AIS ≥3 patients were compared with those from the Deutsche Gesellschaft für Unfallchirurgie Trauma Register. Subsequently, the association between PRF and mortality was assessed for 2AIS ≥3-DNTR patients and compared with those with an ISS of ≥16. RESULTS:The DNTR recorded 300,649 acute trauma admissions. A total of 15,711 patients sustained an ISS of ≥16, and 6,263 patients had suffered a 2AIS ≥3 injury. All individual PRFs were associated with a mortality of {$>$}30\% in 2AIS ≥3-DNTR patients. The increase in PRFs was associated with a significant increase in mortality for both 2AIS ≥3 and ISS ≥16 patients. A total of 4,264 patients met the BPDs criteria. Overall mortality (27.2\%), intensive care unit admission (71.2\%), and length of stay were the highest for the BPD group. CONCLUSION:This study confirms that the BPD identifies high-risk patients in a population-based registry. The addition of PRFs to the anatomical injury scores improves the identification of severely injured patients with a high risk of mortality. Compared with the ISS ≥16 and 2AIS ≥3 multiple injury definitions, the BPD showed to improve the accuracy of capturing patients with a high medical resource need and mortality rate.},
