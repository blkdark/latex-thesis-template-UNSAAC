@article{zhangExtentCompassionSatisfaction2018,
  title = {Extent of Compassion Satisfaction, Compassion Fatigue and Burnout in Nursing: {{A}} Meta-Analysis},
  shorttitle = {Extent of Compassion Satisfaction, Compassion Fatigue and Burnout in Nursing},
