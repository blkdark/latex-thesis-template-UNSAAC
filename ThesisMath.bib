@online{cookScienceHardSoft2023,
  title = {Science {{Hard}} and {{Soft}}: {{An Empirical Approach}} to {{Evaluating Scientific Consensus}} with {{Big Data}}},
  shorttitle = {Science {{Hard}} and {{Soft}}},
  author = {Cook, Gavin G. and Xie, Yu},
  date = {2023-03-05},
  doi = {10.31235/osf.io/zu4bm},
  url = {https://osf.io/zu4bm},
  urldate = {2024-10-24},
  abstract = {Though the terms “soft science” and “hard science” have long been used in both popular and academic discourse, many scholars of science studies argue that the hard–soft distinction does not exist. While the two most active fields of science studies, the science of science and the ethnography of science, have largely ignored questions of inter-field difference, the Mertonian school of the sociology of science explored how science varies between fields at length. We return to this age-old question with new data, theoretical approaches, and computational techniques. We first conduct a survey of the educated and general public to determine general perceptions of the hard–soft hierarchy in the sciences. We then merge contemporary advances in science studies with classical work in the sociology of science to create 14 metrics that explore the practice of science using big data. We find that the hard–soft spectrum exists, and public understandings of the spectrum comport almost exactly with empirical measures of hardness. We conclude by illustrating how a focus on inter-field variation in scientific practice can enrich current work on science studies.},
  pubstate = {prepublished},
  file = {C:\Users\blkda\Mi unidad\Academico\Zotero\Colecciones\CienciasComplejidad\ScienceofScience\Cook y Xie_ 2023_Science Hard and Soft An Empirical Approach to Evaluating Scientific Consensus with Big Data.pdf}
