  title = {Women and {{Meritocracy}}. {{Historical Roots}} of a {{Paradigm}} and {{Current Possibilities}} of {{Transformation}}},
  shorttitle = {{{MUJERES Y MERITOCRACIA}}. {{RAÍCES HISTÓRICAS DE UN PARADIGMA Y POSIBILIDADES ACTUALES DE TRANSFORMACIÓN}}},
