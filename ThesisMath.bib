@article{cairoFallasMercadoRegulacion2014,
  title = {Fallas de mercado y regulación económica: ¿la regulación ejercida por el gobierno permite lograr un mejor funcionamiento de los mercados},
  shorttitle = {FALLAS DE MERCADO Y REGULACIÓN ECONÓMICA},
  author = {Cairo, Vladimir Rodríguez},
