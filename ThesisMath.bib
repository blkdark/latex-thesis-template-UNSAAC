@article{karEvolutionaryGameTheoreticFoundations2017,
  title = {The {{Evolutionary Game-Theoretic Foundations}} of {{Law}}},
  author = {Kar, Robin Bradley},
  date = {2017},
  journaltitle = {Law \& Social Inquiry},
  shortjournal = {Law soc. inq.},
  volume = {42},
  number = {01},
