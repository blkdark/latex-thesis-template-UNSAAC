@book{bosquegonzalezSistemasInformacionGeografica2012,
  title = {Los sistemas de información geográfica y la investigación en ciencias humanas y sociales},
  author = {family=Bosque González, given=Isabel, prefix=del, useprefix=false and Fernández Freire, Carlos and Martín-Forero Morente, Lourdes and Pérez Asensio, Esther},
  date = {2012},
  publisher = {Confederación Española de Centros de Estudios Locales, Consejo Superior de Investigaciones Científicas},
  location = {Madrid},
  abstract = {Casi todos los fenómenos, eventos u objetos de estudio de las Ciencias Humanas y Sociales se producen en un espacio geográfico determinado y pueden ser georreferenciados mediante un sistema de coordenadas espacio-temporal. En este marco, las tecnologías de información geográfica pueden mejorar notablemente la investigación científica en estas disciplinas abriendo otras perspectivas u oportunidades de conocimiento, planteando nuevas cuestiones, aplicando diferentes metodologías de análisis y revelando nuevos datos que sin el uso de estas tecnologías quizás permanecerían invisibles, en definitiva, facilitando la aparición de nuevas interpretaciones de la misma realidad. Su aplicación es relativamente reciente en este campo científico, aunque suficientemente probada en otras ciencias como las relacionadas con los recursos naturales, las problemáticas medioambientales o las ciencias agrarias, por ejemplo. Proyectos del CCHS, como los que se mencionan en esta obra, en el ámbito de la Arqueología, la Historia, la Lengua, la Demografía o las Ciencias Políticas han ido incorporando en los últimos años, de forma novedosa en muchas ocasiones, estas técnicas de investigación basadas en los SIG y aplicando un enfoque de análisis básicamente espacial, favoreciendo un marco de trabajo integrador que facilita la generación de conocimiento científico, a la vez que posibilita la interdisciplinaridad y la creación de sinergias en la investigación en Ciencias Humanas y Sociales. El propósito de esta obra es adentrarse en la teoría y los fundamentos de los Sistemas de Información Geográfica (SIG) desde la perspectiva de su implementación en las Ciencias Humanas y Sociales. En este libro se presentan los fundamentos básicos para el diseño y el modelado de datos espaciales y la generación de bases de datos georreferenciadas, se abordan las funciones elementales de consulta y edición, análisis espacial y representación de la información mediante técnicas cartográficas, para terminar con su difusión y publicación. Esperamos proporcionar unas pautas que permitan la comprensión de lo que es un SIG, cómo funciona y cómo su aplicabilidad técnica puede incidir favorablemente en la actividad investigadora en estas disciplinas},
  isbn = {978-84-615-9825-0},
  langid = {spanish},
  annotation = {OCLC: 955679563},
  file = {C:\Users\blkda\Mi unidad\Academico\Zotero\Colecciones\CienciasSociales\Antropologia\Bosque González et al._ 2012_Los sistemas de información geográfica y la investigación en ciencias humanas y sociales.pdf}
